{
  "name": "tree-sitter-ada",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "tree-sitter": "0.20.6",
    "tree-sitter-cli": "0.20.8",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter generate && time tree-sitter test",
    "testquick": "time tree-sitter test",
    "update-test": "tree-sitter test --update",
    "local_install": "cp queries/*scm ~/.local/share/nvim/site/pack/packer/start/nvim-treesitter/queries/ada; echo 'RUN :TSInstall ada' in nvim",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.ada",
      "file-types": [
        "adb",
        "ads"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "devDependencies": {
    "prebuildify": "^6.0.0"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
